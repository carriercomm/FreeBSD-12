include ../Makefiles/git1.mk

all:
	@echo "This is the Makefile only for committing to GitHub. Use make -f not_makefile"

include ../Makefiles/git2.mk

vi:
	vi $(documentation)

notes:
	(cd ../notes && make vi)

quotes:
	(cd ../notes && make quotes)

bibtex:
	(cd ../bibtex && make vi)

spell:
	aspell --lang=en check $(documentation)

clean:
	rm -f $(documentation).bak

